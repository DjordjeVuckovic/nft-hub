{
  "name": "@cbs/nft-hub",
  "version": "0.0.1",
  "description": "NFT hub",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:all": "npm-run-all --parallel --continue-on-error dev:be dev:fe",
    "dev:be": "npm run start:dev --workspace=@cbs/nft-hub-be",
    "dev:fe": "npm run dev --workspace=@cbs/nft-hub-fe"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.0"
}
